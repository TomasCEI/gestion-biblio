#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/xampp/htdocs/ALUMNOS/CEI-full-stack/gestion-biblio/back/node_modules/nopt/bin/node_modules:/mnt/c/xampp/htdocs/ALUMNOS/CEI-full-stack/gestion-biblio/back/node_modules/nopt/node_modules:/mnt/c/xampp/htdocs/ALUMNOS/CEI-full-stack/gestion-biblio/back/node_modules:/mnt/c/xampp/htdocs/ALUMNOS/CEI-full-stack/gestion-biblio/node_modules:/mnt/c/xampp/htdocs/ALUMNOS/CEI-full-stack/node_modules:/mnt/c/xampp/htdocs/ALUMNOS/node_modules:/mnt/c/xampp/htdocs/node_modules:/mnt/c/xampp/node_modules:/mnt/c/node_modules:/mnt/c/xampp/htdocs/ALUMNOS/CEI-full-stack/gestion-biblio/back/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/xampp/htdocs/ALUMNOS/CEI-full-stack/gestion-biblio/back/node_modules/nopt/bin/node_modules:/mnt/c/xampp/htdocs/ALUMNOS/CEI-full-stack/gestion-biblio/back/node_modules/nopt/node_modules:/mnt/c/xampp/htdocs/ALUMNOS/CEI-full-stack/gestion-biblio/back/node_modules:/mnt/c/xampp/htdocs/ALUMNOS/CEI-full-stack/gestion-biblio/node_modules:/mnt/c/xampp/htdocs/ALUMNOS/CEI-full-stack/node_modules:/mnt/c/xampp/htdocs/ALUMNOS/node_modules:/mnt/c/xampp/htdocs/node_modules:/mnt/c/xampp/node_modules:/mnt/c/node_modules:/mnt/c/xampp/htdocs/ALUMNOS/CEI-full-stack/gestion-biblio/back/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nopt/bin/nopt.js" "$@"
else
  exec node  "$basedir/../nopt/bin/nopt.js" "$@"
fi
